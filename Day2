}
}

public class Question6 {
	public static void main(String[] args) {

	String name0, name1, name2, name3, name4;
	name0="James";
	name1="Jill";
	name2="Betty";
	name3="Herb";
	name4="Bob";
	System.out.println(name4+name0+name2+name3+name1);

}
}

public class Question7 {
	public static void main(String[] args) {
	String i="9";
	float f=33.4f;
	System.out.println("i="+i+" f="+f);


}
}

