public class Question10 {
	public static void main(String[] args) {
	int hour, minute, second;
	String amOrPm="PM";
	hour=12;
	minute=24;
	second=33;
	System.out.println(hour+":"+minute+":"+second+ " "+ amOrPm);
}
}
